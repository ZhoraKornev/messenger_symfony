{
    "type": "project",
    "license": "proprietary",
    "require": {
        "php": "^8.1",
        "ext-ctype": "*",
        "ext-iconv": "*",
        "composer/package-versions-deprecated": "@dev",
        "doctrine/annotations": "@dev",
        "doctrine/doctrine-bundle": "@dev",
        "doctrine/doctrine-cache-bundle": "^1.3",
        "doctrine/doctrine-migrations-bundle": "@dev",
        "doctrine/orm": "@dev",
        "intervention/image": "@dev",
        "phpdocumentor/reflection-docblock": "@dev",
        "sensio/framework-extra-bundle": "@dev",
        "symfony/console": "@dev",
        "symfony/dotenv": "@dev",
        "symfony/flex": "@dev",
        "symfony/framework-bundle": "@dev",
        "symfony/property-access": "@dev",
        "symfony/property-info": "@dev",
        "symfony/serializer": "@dev",
        "symfony/validator": "@dev",
        "symfony/webpack-encore-bundle": "@dev",
        "symfony/yaml": "@dev"
    },
    "config": {
        "preferred-install": {
            "*": "dist"
        },
        "sort-packages": true,
        "platform": {
            "php": "8.1.9"
        },
        "allow-plugins": {
            "symfony/flex": true,
            "symfony/runtime": true
        }
    },
    "autoload": {
        "psr-4": {
            "App\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "App\\Tests\\": "tests/"
        }
    },
    "replace": {
        "paragonie/random_compat": "2.*",
        "symfony/polyfill-ctype": "*",
        "symfony/polyfill-iconv": "*",
        "symfony/polyfill-php71": "*",
        "symfony/polyfill-php70": "*",
        "symfony/polyfill-php56": "*"
    },
    "scripts": {
        "auto-scripts": {
            "cache:clear": "symfony-cmd",
            "assets:install %PUBLIC_DIR%": "symfony-cmd"
        },
        "post-install-cmd": [
            "@auto-scripts"
        ],
        "post-update-cmd": [
            "@auto-scripts"
        ]
    },
    "conflict": {
        "symfony/symfony": "*"
    },
    "extra": {
        "symfony": {
            "allow-contrib": false,
            "require": "@dev"
        }
    },
    "require-dev": {
        "easycorp/easy-log-handler": "@dev",
        "symfony/debug-bundle": "@dev",
        "symfony/maker-bundle": "@dev",
        "symfony/monolog-bundle": "@dev",
        "symfony/stopwatch": "@dev",
        "symfony/twig-bundle": "@dev",
        "symfony/var-dumper": "@dev",
        "symfony/web-profiler-bundle": "@dev"
    }
}
